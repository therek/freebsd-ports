#!/bin/sh
# $FreeBSD: ports/x11/gdm/files/gdm.sh.in,v 1.6 2005/11/25 18:34:34 marcus Exp $

# PROVIDE: gdm
# REQUIRE: DAEMON LOGIN cleanvar
# KEYWORD: FreeBSD

gdm_enable=${gdm_enable-"NO"}

. %%RC_SUBR%%

name=gdm
rcvar=`set_rcvar`
command="%%PREFIX%%/sbin/${name}"
pidfile="/var/run/${name}.pid"
procname="%%PREFIX%%/sbin/gdm-binary"

load_rc_config ${name}
run_rc_command "$1"
